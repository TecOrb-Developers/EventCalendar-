    <style name="CalendarCard"/>
    <style name="CalendarCard.CellTextView">
        <item name="android:layout_marginLeft">2dp</item>
        <item name="android:layout_marginRight">2dp</item>
        <item name="android:textColor">@color/calendar_cell_text_color</item>
        <item name="android:textSize">@dimen/text_size_medium</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/background_drawable</item>
        <item name="android:fontFamily">sans-serif-condensed</item>
    </style>
    <style name="CalendarCard.Day">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">@color/grey_black</item>
        <item name="android:fontFamily">sans-serif-condensed</item>
    </style>
    <declare-styleable name="CountDownTimerView">
        <attr format="integer" name="maxTimeInSeconds"/>
        <attr format="integer" name="intervalInSeconds"/>
    </declare-styleable>
    <declare-styleable name="WeekView">
        <attr format="enum" name="firstDayOfWeek">
            <enum name="sunday" value="1"/>
            <enum name="monday" value="2"/>
            <enum name="tuesday" value="3"/>
            <enum name="wednesday" value="4"/>
            <enum name="thursday" value="5"/>
            <enum name="friday" value="6"/>
            <enum name="saturday" value="7"/>
